{
  "cwd": "..",
  "env": {},
  "disabled": [
    "//atom",
    "//chromium",
    "//git",
    "//launch",
    "//lazygit",
    "//node",
    "//npm",
    "//openssl",
    "//smartgit",
    "//vscode"
  ],
  "versions": {
    "7zip": "1801",
    "node": "",
    "npm": ""
  },
  "path": [],
  "scripts": {
    "paltry-cleanup": [
      "powershell -ExecutionPolicy Bypass -File %PALTRY_HOME%\\src\\cleanup.ps1"
    ],
    "paltry-format": [
      "powershell -ExecutionPolicy Bypass -File %PALTRY_HOME%\\src\\format.ps1"
    ]
  },
  "git": {
    "ssh": true,
    "repos": {}
  },
  "vscode": {
    "extensions": [
      "christian-kohler.path-intellisense",
      "wayou.vscode-todo-highlight",
      "eamodio.gitlens"
    ],
    "settings": {
      "editor.tabSize": 2,
      "editor.detectIndentation": false,
      "files.eol": "\n",
      "editor.formatOnSave": true,
      "explorer.openEditors.visible": 0,
      "files.autoSave": "onFocusChange",
      "workbench.editor.enablePreview": false
    }
  }
}